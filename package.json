{
  "name": "is-email-valid",
  "version": "1.0.1",
  "description": "Check if the email is valid",
  "main": "index.umd.js",
  "scripts": {
    "test": "ava test.js",
    "build": "rollup index.js --output.format umd --name \"is-email-valid\" --output.file index.umd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fintory/is-email-valid.git"
  },
  "keywords": [
    "email",
    "validator",
    "regex",
    "checker"
  ],
  "author": "Nick Schmidt <nick@fintory.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fintory/is-email-valid/issues"
  },
  "homepage": "https://github.com/fintory/is-email-valid#readme",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "rollup": "^0.52.3"
  },
  "ava": {
    "files": [
      "test.js"
    ],
    "concurrency": 2,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
